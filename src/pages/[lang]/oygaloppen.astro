---
import OygaloppenPage from "../../components/OygaloppenPage.astro";

export function getStaticPaths() {
  return [{ params: { lang: "no" } }, { params: { lang: "en" } }];
}

const { lang } = Astro.params;

// Validate language parameter
if (lang !== "no" && lang !== "en") {
  return Astro.redirect("/no/oygaloppen");
}
---

<OygaloppenPage />
